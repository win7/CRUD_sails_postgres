<div class="table-responsive">
	<table class="table table-striped">
	  <thead>
	    <tr>
	    	<th> Id </th>
	      <th> Name </th>
	      <th> Last name </th>
	      <th> Username </th>
	      <th> Email </th>
	      <th> Birth </th>
	      <th> Phone </th>
	      <th> Image </th>
	      <th> Remove </th>
	    </tr>
	  </thead>
	  <tbody>
	      <% for(var i = 0; i < contacts.length; i++) {
	          var item = contacts[i];
	      %>
	      <tr>
	        <td><a href="/find_one/<%= item.id %>"> <%= item.id %> </a></td>
	        <td> <%= item.name %> </td>
	        <td> <%= item.lastname %> </td>
	        <td> <%= item.username %> </td>
	        <td> <%= item.email %> </td>
	        <td> <%= item.birth %> </td>
	        <td> <%= item.phone %> </td>
	        <td><img src="<%= item.image_url %>" alt="<%= item.image_url %>" class="img-circle" width="50px" height="50px"></td>
	        <td>
	          <form action="/delete_contact" method="post">
	            <input type="hidden" name="_method" value="delete">
	            <input type="hidden" name="id" value="<%= item.id %>">
	            <button type="submit" class="btn btn-danger"> Delete </button>
	          </form>
	        </td>
	      </tr>
	      <% } %>
	  </tbody>
	</table>
</div>